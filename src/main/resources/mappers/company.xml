<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.winiaaid.domain.company.CompanyRepository">
    <select id="findCompanyList" resultType="com.project.winiaaid.domain.company.Company">
        SELECT
            company_code,
            company_name
        FROM
            product_company_table;
    </select>
</mapper>